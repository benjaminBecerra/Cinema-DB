ia }) => {\n\n  let params = useParams()\n  let navigate = useNavigate()\n\n  const handleScroll = (med) => {\n    if (params !== \"/\") navigate(\"/\")\n    if (med !== media) handleMediaChange(media)\n    setTimeout(() => {\n      window.scrollTo({ top: 1850, behavior: 'smooth' })\n    }, 500)\n\n  }\n\n  const handleScrollTop = () => {\n    if (params !== \"/\") navigate(\"/\")\n     setTimeout(() => {\n      window.scrollTo({ top: 0, behavior: 'smooth' })\n    }, 500)\n  }\n\n\n  return (\n    <nav class=\"navbar is-primary is-fixed-top\" role=\"navigation\" aria-label=\"main navigation\" style={{ boxShadow: \"0 2px 4px 0 rgba(0,0,0,.2)\" }}>\n      <div class=\"navbar-brand\">\n        <Link to=\"/\" class=\"navbar-item\">\n          <img src=\"https://i.postimg.cc/R0SznM9x/CINEMADB.png\" width=\"112\" height=\"28\" alt=\"\" />\n        </Link>\n\n        <a role=\"button\" class=\"navbar-burger\" aria-label=\"menu\" aria-expanded=\"false\" data-target=\"navbarBasicExample\">\n          <span aria-hidden=\"true\"></span>\n          <span aria-hidden=\"true\"></span>\n          <span aria-hidden=\"true\"></span>\n        </a>\n      </div>\n\n      <div id=\"navbarBasicExample\" class=\"navbar-menu\">\n        <div class=\"navbar-start\">\n\n          <a class=\"navbar-item\"  onClick={() => handleScrollTop()}>\n            Home\n          </a>\n\n         { user && <Link to=\"/favourites\" class=\"navbar-item\">\n            <p>\n              Favoritos\n            </p>\n          </Link>}\n\n          <a class=\"navbar-item\" onClick={() => handleScroll(\"movie\")}>\n            Peliculas\n          </a>\n\n          <a class=\"navbar-item\" onClick={() => handleScroll(\"tv\")}>\n              Programas de TV\n          </a>\n\n\n          <div class=\"navbar-item has-dropdown is-hoverable\">\n            <p class=\"navbar-link\">\n              Mas...\n            </p>\n\n            <div class=\"navbar-dropdown\">\n              <Link to=\"/404\" class=\"navbar-item\">\n                <p>\n                  Jobs\n                </p>\n              </Link>\n              <Link to=\"/404\" class=\"navbar-item\">\n                <p>\n                  Contact\n                </p>\n              </Link>\n\n              <hr class=\"navbar-divider\" />\n              <Link to=\"/404\" class=\"navbar-item\">\n                <p>\n                  Report an issue\n                </p>\n              </Link>\n\n            </div>\n          </div>\n        </div>\n\n\n        <div class=\"navbar-end\">\n          <div class=\"navbar-item\">\n\n            {user ?\n              <div class=\"buttons\">\n                <div style={{ paddingRight: 13 }}>\n                  <p class=\"title is-6\">{user.email}</p>\n                </div>\n                <p class=\"button is-light\" onClick={handleLogOut}>\n                  Log out\n                </p>\n              </div>\n              :\n\n              <div class=\"buttons\">\n\n                <Link to=\"/signup\">\n                  <a href=\"\" class=\"button is-primary\">\n                    <strong>Sign up</strong>\n                  </a>\n                </Link>\n\n                <Link to=\"/login\">\n                  <a href=\"/\" class=\"button is-light\">\n                    Log in\n                  </a>\n                </Link>\n\n              </div>}\n          </div>\n        </div>\n      </div>\n    </nav>\n  )\n}\n\nexport default Navbar","/Users/benja/Desktop/Bootcamp/Cinema-DB/src/components/notFound.js",[],[],"/Users/benja/Desktop/Bootcamp/Cinema-DB/src/components/movie.js",["296"],[],"import dateNormalization from \"../utils/dateNormalization\"\nimport { AiFillStar } from \"react-icons/ai\";\nimport handleFavourites from \"../utils/handleFavourites\";\n\n\nconst Movie = ({ movie, user, setUser }) => {\n\n\n  const setColor = () => {\n    if (user) {\n      if (user.favourites.length) return user.favourites.find((mov) => mov.movie_id == movie.id)\n    }\n  }\n\n\n  return (\n    <section class=\"hero is-small is-primary\">\n      <div class=\"hero-body\">\n        <div class=\"columns is-multiline\">\n          <div class=\"column is-one-third\" >\n            <img style={{ position: \"relative\" }} class=\"image is-2by4\" src={`https://image.tmdb.org/t/p/w500${movie.poster_path}`} alt=\"movie poster\" />\n            { user && <AiFillStar onClick={() => handleFavourites(movie, setUser, user)} style={{ position: \"absolute\", top: \"2%\", left: \"90%\", zIndex: \"1\", fontSize: \"40px\", color: setColor() ? \"gold\" : \"black\" }} />}\n          </div>\n          <div class=\"column is-two-thirds\" style={{ padding: \"0 2%\" }}>\n            <p class=\"title is-1\" style={{ textAlign: \"center\", margin: '0 auto' }}>{movie.name ? `${movie.name}` : `${movie.title} `}</p>\n            <p class=\"title is-3\" style={{ textAlign: \"center\" }}>{movie.name ? `(${movie.original_name})` : `(${movie.original_title})`}</p>\n            <div class=\"columns\">\n              <div class=\"column is-8\">\n                <p class=\"title is-6\">{movie.genres.map(genre => genre.name + \" - \")}{movie.runtime + \" mins.\"}</p>\n              </div>\n              <div class=\"column is-4\">\n                <p class=\"title is-6\"> Estreno: {movie.first_air_date ? dateNormalization(movie.first_air_date) : dateNormalization(movie.release_date)}</p>\n              </div>\n            </div>\n\n            <div style={{ backgroundColor: \"rgba(0, 0, 0, 0.5)\", borderRadius: \"10px\", margin: \"5% 0\" }}>\n              <div class=\"column is-half is-offset-one-third\">\n                <p class=\"title is-5\">{movie.tagline}</p>\n              </div>\n\n              <div class=\"column is-full mt-2 mr-0\" style={{ textAlign: \"center\" }}>\n                {movie.overview}\n              </div>\n            </div>\n\n            <div class=\"columns\">\n              <div class=\"column is-half\">\n                <div class=\"box\" style={{ height: '70%', maxWidth: \"90%\", margin: \"5% 0\" }}>\n                  <div>\n                    <progress class=\"progress is-link mb-1\" value={movie.vote_average} max=\"10\"></progress>\n                  </div>\n                  <div class=\"content\">\n                    <h3>Puntuacion de usuarios: {String(movie.vote_average).slice(0, 3)} %</h3>\n                    <p>Cantidad de votos: {movie.vote_count}</p>\n                  </div>\n                </div>\n              </div>\n              <div class=\"column is-half\" >\n                <div class=\"box content\" style={{ height: '70%', maxWidth: \"90%\", margin: \"5% 0\", textAlign: \"center\" }}>\n\n                  {movie.production_companies ? movie.production_companies.map((prod) => <h5>{prod.name}</h5>): \"\"} \n\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n  )\n}\n\nexport default Movie","/Users/benja/Desktop/Bootcamp/Cinema-DB/src/components/grid.js",[],[],"/Users/benja/Desktop/Bootcamp/Cinema-DB/src/components/cast.js",[],[],"/Users/benja/Desktop/Bootcamp/Cinema-DB/src/commons/modalCard.js",["297","298"],[],"import dateNormalization from \"../utils/dateNormalization\"\nimport { Link } from \"react-router-dom\";\nimport { AiFillStar } from \"react-icons/ai\";\nimport handleFavourites from \"../utils/handleFavourites\";\n\nconst ModalCard = ({ data, user, setUser, setActive }) => {\n\n const setColor = () => {\n    if(user){\n   if(user.favourites.length) return user.favourites.find((mov) => mov.movie_id == data.id)}\n  }\n\n  return (\n    <div class=\"card\">\n\n       { user && <AiFillStar style={{ fontSize: \"40px\", position: \"absolute\", top: \"2%\", left: \"90%\", zIndex: \"1\", color: setColor() ? \"gold\" : \"white\" }} onClick={() => handleFavourites(data, setUser, user)} />}\n\n      <div class=\"card-image\">\n        <figure class=\"image is-4by2\">\n          <img src={`https://image.tmdb.org/t/p/w500${data.backdrop_path}`} alt=\"\" />\n        </figure>\n      </div>\n      <div class=\"card-content\">\n        <div class=\"media\">\n          <div class=\"media-center\">\n          </div>\n          <div class=\"media-content\">\n            <p class=\"title is-3\">{data.name ? `${data.name} (${data.original_name})` : `${data.title} (${data.original_title})`}</p>\n            <p>{data.tagline}</p>\n          </div>\n        </div>\n\n        <div class=\"content\">\n          Estreno: {data.first_air_date ? dateNormalization(data.first_air_date) : dateNormalization(data.release_date)}\n\n          <p class=\"title is-5 is-box\">Votos de la gente: {String(data.vote_average).slice(0,3)}%</p>\n        </div>\n        <p>{data.overview}</p>\n        <Link to=\"/item\">\n          <a href=\"\" onClick={() => setActive(\"\")}>Leer mas...</a>\n        </Link>\n      </div>\n    </div>\n\n  )\n}\n\nexport default ModalCard","/Users/benja/Desktop/Bootcamp/Cinema-DB/src/commons/notification.js",[],[],"/Users/benja/Desktop/Bootcamp/Cinema-DB/src/commons/card.js",["299"],[],"/Users/benja/Desktop/Bootcamp/Cinema-DB/src/utils/dateNormalization.js",[],[],"/Users/benja/Desktop/Bootcamp/Cinema-DB/src/commons/castCard.js",[],[],"/Users/benja/Desktop/Bootcamp/Cinema-DB/src/components/footer.js",[],[],"/Users/benja/Desktop/Bootcamp/Cinema-DB/src/components/favourites.js",["300","301"],[],"import Grid from \"./grid\"\nimport axios from \"axios\"\nimport { useState,useEffect } from \"react\"\n\nconst Favourites = ({ handleIdSearch, user, setUser }) => {\n\n  const [favs, setFavs] = useState([])\n  let url = \"http://localhost:3000/api\"\n\n  useEffect(() => {\n    let favoritos = []\n    user.favourites.forEach(mov => {\n      axios.get(`${url}/cont/search/${mov.movie_media}/${mov.movie_id}`)\n        .then(res => favoritos = [...favoritos, res.data])\n        .then(()=> setFavs(favoritos))    \n    });\n  }, [user])\n\n  return (\n    user ? <div s